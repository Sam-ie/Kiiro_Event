<?xml version="1.0" encoding="utf-8" ?>
<Defs> 
  
  
  <!--信使-->
  <PawnKindDef ParentName="Kiiro_PawnBase">
    <defName>Kiiro_Messenger</defName>
    <label>绮罗 信使</label>
    <combatPower>60</combatPower>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
	<factionLeader>false</factionLeader>
    <apparelMoney>
      <min>100</min>
      <max>500</max>
    </apparelMoney>
    <apparelTags>
	  <li>Kiiro_DailyWear</li>
	  <li>Kiiro_Underwear</li>
	  <li>Kiiro_DailyCoat</li>
	  <li>Kiiro_DailyHat</li>
	  <li>Kiiro_Bag</li>
	  <li>Kiiro_Neck</li>
    </apparelTags>
	<specificApparelRequirements>
      <li>
        <bodyPartGroup>Legs</bodyPartGroup>
        <requiredTag>Kiiro_Pants</requiredTag>
      </li>
	  <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <requiredTag>Kiiro_DailyWear</requiredTag>
      </li>
	  <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <requiredTag>Kiiro_Underwear</requiredTag>
      </li>
	  <li>
        <bodyPartGroup>Shoulders</bodyPartGroup>
        <requiredTag>Kiiro_Messenger</requiredTag>
      </li>
    </specificApparelRequirements>
	<apparelDisallowTags>
    </apparelDisallowTags>
    <apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
	<weaponTags>
	  <li>Kiiro_CrossBow</li>
    </weaponTags>
	<weaponMoney>
      <min>400</min>
      <max>1000</max>
    </weaponMoney>
    <backstoryFiltersOverride>
	  <li>
		<categories>
		  <li>Kiiro_Messenger</li>
		</categories>
	  </li>
	</backstoryFiltersOverride>
  </PawnKindDef>
  
